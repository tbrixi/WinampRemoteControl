<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:mx="library://ns.adobe.com/flex/mx"
		width.disabled="32" height.disabled="32">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>

	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="down" />
		<s:State name="over" />
		<s:State name="up" />
	</s:states>

	<!-- SkinParts
	name=labelDisplay, type=spark.components.supportClasses.TextBase, required=false
	-->

	<s:BitmapImage source="@Embed('assets/pause_up.png')"
				   source.over="@Embed('assets/pause_over.png')"
				   source.down="@Embed('assets/pause_over.png')"
				   left="0" right="0" top="0" bottom="0" />

</s:Skin>
